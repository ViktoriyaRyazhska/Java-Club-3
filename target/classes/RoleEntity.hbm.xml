<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team1.entity.Role" table="role" schema="library">
        <id name="id">
            <column name="id" sql-type="int unsigned"/>
        </id>
        <property name="roleName">
            <column name="role_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <set name="usersById" inverse="true">
            <key>
                <column name="role_id"/>
            </key>
            <one-to-many not-found="ignore" class="team1.entity.User"/>
        </set>
    </class>
</hibernate-mapping>